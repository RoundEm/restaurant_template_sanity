{
  "name": "restauranttemplate",
  "private": true,
  "version": "1.0.0",
  "description": "My Next.js/Sanity.io template for restaurant websites",
  "main": "package.json",
  "author": "JASON ROUNDTREE <jasontree123@gmail.com>",
  "scripts": {
    "dev": "next",
    "build": "npm run export",
    "start": "next start",
    "test": "jest --coverage",
    "export": "next build && next export"
  },
  "keywords": [
    "menu",
    "restaurant",
    "food"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "dependencies": {
    "@sanity/base": "^2.1.6",
    "@sanity/client": "^2.1.4",
    "@sanity/components": "^2.1.4",
    "@sanity/core": "^2.1.4",
    "@sanity/default-layout": "^2.1.6",
    "@sanity/default-login": "^2.1.4",
    "@sanity/desk-tool": "^2.1.6",
    "@sanity/vision": "^2.1.4",
    "@zeit/next-css": "^1.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^11.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sanity-plugin-dashboard-widget-notes": "^0.3.2",
    "sanity-plugin-seo-tools": "^1.0.6",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^26.6.3",
    "webpack": "^4.44.0"
  }
}
